 {
   "RECIPE_ID": 28,
   "COOKING_NO": 2,
   "COOKING_DC": "방울토마토는 반을 잘라둔다.",
   "STRE_STEP_IMAGE_URL": "",
   "STEP_TIP": ""
 },
 {
   "RECIPE_ID": 28,
   "COOKING_NO": 3,
